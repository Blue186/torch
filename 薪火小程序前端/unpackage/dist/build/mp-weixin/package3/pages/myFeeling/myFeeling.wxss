.page{background-color:#f8f8f8;padding:20rpx 25rpx}.page .container{padding:40rpx 40rpx;background-color:#fff;box-shadow:3rpx 3rpx 10rpx 8rpx #eee;border-radius:20rpx}.page .container .title{font-size:40rpx;font-weight:550}.page .container .evaluate{margin:30rpx 40rpx}.page .container .evaluate .rate{display:flex}.page .container .evaluate .rate text{margin-right:20rpx}.page .container .feeling{margin-bottom:30rpx}.page .container .images{overflow:hidden;margin-bottom:100rpx;display:flex;justify-content:flex-start;align-items:center;flex-wrap:wrap}.page .container .images .entry{width:180rpx;height:180rpx;margin-left:30rpx;background-size:contain;background-repeat:no-repeat;background-image:url(data:image/png;base64,)}.page .container .images .image{margin-bottom:20rpx;border:1rpx solid #000}.page .container .images .image image{width:180rpx;height:180rpx}.page .container .images .image:nth-child(3n+2){margin-left:30rpx;margin-right:30rpx}